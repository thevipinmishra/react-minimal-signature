#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/vipin/Workspace/Personal/open-source/react-minimal-signature/node_modules/.pnpm/tsup@8.2.4_jiti@1.21.6_postcss@8.4.41_typescript@5.5.4_yaml@2.5.0/node_modules/tsup/dist/node_modules:/home/vipin/Workspace/Personal/open-source/react-minimal-signature/node_modules/.pnpm/tsup@8.2.4_jiti@1.21.6_postcss@8.4.41_typescript@5.5.4_yaml@2.5.0/node_modules/tsup/node_modules:/home/vipin/Workspace/Personal/open-source/react-minimal-signature/node_modules/.pnpm/tsup@8.2.4_jiti@1.21.6_postcss@8.4.41_typescript@5.5.4_yaml@2.5.0/node_modules:/home/vipin/Workspace/Personal/open-source/react-minimal-signature/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/vipin/Workspace/Personal/open-source/react-minimal-signature/node_modules/.pnpm/tsup@8.2.4_jiti@1.21.6_postcss@8.4.41_typescript@5.5.4_yaml@2.5.0/node_modules/tsup/dist/node_modules:/home/vipin/Workspace/Personal/open-source/react-minimal-signature/node_modules/.pnpm/tsup@8.2.4_jiti@1.21.6_postcss@8.4.41_typescript@5.5.4_yaml@2.5.0/node_modules/tsup/node_modules:/home/vipin/Workspace/Personal/open-source/react-minimal-signature/node_modules/.pnpm/tsup@8.2.4_jiti@1.21.6_postcss@8.4.41_typescript@5.5.4_yaml@2.5.0/node_modules:/home/vipin/Workspace/Personal/open-source/react-minimal-signature/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-default.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-default.js" "$@"
fi
